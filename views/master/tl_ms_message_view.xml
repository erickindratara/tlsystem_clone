 <odoo>
    <data>  <!--Wizard For Raise Apprval Messages Thesis ORIC-->
    <!-- <record id="wizard_message_form_for_oric" model="ir.ui.view"> -->
        <record id="tl_ms_message_view_form" model="ir.ui.view">
                <field name="name">tl.ms.message.form</field>
                <field name="model">tl.ms.message</field>
                <field name="arch" type="xml">
                    <form string="Message">
                        <script>    
                            $(document).ready(function(){
                            $(".modal-header").hide();
                            $(".modal-footer").hide();
                            });
                        </script> 
                        <sheet>
                            <group  col="4" colspan="4">   
                                <field name="text" string="text asu" colspan="4" nolabel="1" readonly="1"  widget="html"/>
                                <button name="btn_approve_oric" type="object" string="Yes" class="oe_highlight"/>                                 
                                <button special="cancel" string="No"/>        
                            </group>
                        </sheet>                                 
                    </form>
                </field>
        </record>  
        
         
        


    </data>
</odoo>
  